{
  "name": "Slector Extension",
  "description": "Configurable Slector Extension",
  "version": "1.0",
  "manifest_version": 3,
  "type": "module",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage", "tabs", "scripting"],
  "host_permissions": ["*://*/"],
  "action": {
    "default_popup": "/popup/popup.html",
    "default_icon": { "128": "/img/icon-128.png" }
  },
  "icons": { "128": "/img/icon-128.png" }
}
